{
    "servers": {
        "laravel-boost": {
            "command": "ddev",
            "args": [
                "art",
                "boost:mcp",
                "--no-ansi",
                "-q"
            ]
        },
        "figma": {
            "url": "https://mcp.figma.com/mcp",
            "type": "http"
        },
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        }
